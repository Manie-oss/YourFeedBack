<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/activity_main">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            tools:ignore="MissingConstraints">
            <TextView
                android:background="@color/skyblue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="GDG FeedBack Form"
                android:textAlignment="center"
                android:textSize="30dp"/>

        </LinearLayout>


        <ScrollView
            android:id="@+id/sview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/blue"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/ll"
                android:background="@drawable/gradient_list"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_marginTop="20dp"
                    android:background="@drawable/linear"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="320dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Your Name"
                            android:textSize="24dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/red"
                            android:text=" *"
                            android:textSize="24dp" />
                    </LinearLayout>
                    <EditText
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/nameedt"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Name..."
                        android:textSize="18dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Email Address"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/email"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="email..."
                        android:textSize="18dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Gender"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                        <LinearLayout
                            android:layout_marginLeft="15dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <RadioGroup
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <RadioButton
                                    android:id="@+id/male"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:text="Male"
                                    android:textSize="18dp"/>
                                <RadioButton
                                    android:id="@+id/female"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:text="Female"
                                    android:textSize="18dp"/>
                                <RadioButton
                                    android:id="@+id/other"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:text="Other"
                                    android:layout_marginBottom="10dp"
                                    android:textSize="18dp"/>
                            </RadioGroup>

                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Age"
                            android:textSize="24dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp" />
                    </LinearLayout>

                    <SeekBar
                        android:id="@+id/sage"
                        android:layout_width="315dp"
                        android:layout_height="wrap_content"
                        android:max="100"
                        android:min="1" />

                    <TextView
                        android:id="@+id/tage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:textAlignment="center"
                        android:layout_gravity="center"
                        android:textSize="24dp"
                        android:layout_marginBottom="10dp"
                        android:visibility="visible" />

                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="College/Organisation"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginBottom="10dp"
                        android:id="@+id/clg"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:hint="College Name..."
                        android:textSize="18dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Year of Study"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <Spinner
                        android:layout_width="320dp"
                        android:id="@+id/spinner"
                        android:entries="@array/year"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Mobile Number"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginBottom="10dp"
                        android:id="@+id/mob"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Mobile Number..."
                        android:inputType="number"
                        android:textSize="18dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="What's your Area of Expertise?"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <CheckBox
                        android:id="@+id/e1"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Reaching Out(Companies/Colleges)"/>
                    <CheckBox
                        android:id="@+id/e2"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Designing"/>
                    <CheckBox
                        android:id="@+id/e3"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Marketing"/>
                    <CheckBox
                        android:id="@+id/e4"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Writing"/>
                    <CheckBox
                        android:id="@+id/e5"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Programming"/>
                    <CheckBox
                        android:id="@+id/e6"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Management"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Technologies You are good at?"
                            android:textSize="24dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp"/>
                    </LinearLayout>
                    <CheckBox
                        android:id="@+id/t1"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Python"/>
                    <CheckBox
                        android:id="@+id/t2"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" JavaScript"/>
                    <CheckBox
                        android:id="@+id/t3"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" React"/>
                    <CheckBox
                        android:id="@+id/t4"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Angular"/>
                    <CheckBox
                        android:id="@+id/t5"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" HTML + CSS"/>
                    <CheckBox
                        android:id="@+id/t6"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" PHP"/>
                    <CheckBox
                        android:id="@+id/t7"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Node.js"/>
                    <CheckBox
                        android:id="@+id/t8"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" SQL/NoSQL"/>
                    <CheckBox
                        android:id="@+id/t9"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" C#"/>
                    <CheckBox
                        android:id="@+id/t10"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Go"/>
                    <CheckBox
                        android:id="@+id/t11"
                        android:layout_marginTop="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18dp"
                        android:text=" Other"/>
                    <EditText
                        android:id="@+id/t12"
                        android:layout_marginTop="15dp"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Other Technology"
                        android:textSize="24dp"
                        android:layout_marginBottom="10dp"/>
                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Specialisation you are working in?"
                            android:textSize="24dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" *"
                            android:textColor="@color/red"
                            android:textSize="24dp" />
                    </LinearLayout>

                    <CheckBox
                        android:id="@+id/s1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Web Dovelopment"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Android Development"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Software Development"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Data Science/ML/DL"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" BlockChain"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Cloud Computing"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" DevOps"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Quantum Computing"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Competitive Programming"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Product Development"
                        android:textSize="18dp" />

                    <CheckBox
                        android:id="@+id/s11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text=" Cyber Security"
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="LinkedIn Profile"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/lpro"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Profile..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Facebook Profile"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/fb"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:hint="Profile..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="GitHub Profile"
                            android:layout_marginLeft="10dp"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/ghp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:hint="GitHub Profile..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Link to your Resume"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/rl"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:hint="Resume..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Why do you want to join VIP Programme and be a Volunteer?"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/join"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Join..?"
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Share your past experience in which you have worked aling side a community?"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/pex"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Experience..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Anything you'd like to tell us about you?"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/tell"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Answer here..."
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Name of the person who reffered you for this?"
                            android:textSize="24dp"/>

                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="20dp"
                        android:id="@+id/ref"
                        android:layout_width="300dp"
                        android:layout_height="wrap_content"
                        android:hint="Person Name"
                        android:textSize="18dp"
                        android:layout_marginBottom="10dp"/>

                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/linear"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <CheckBox
                        android:id="@+id/tc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text="I agree with the terms and conditions of GDG." />

                    <Button
                        android:id="@+id/next"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:text="Next>>"
                        android:textSize="24dp" />
                </LinearLayout>


            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>